{
  "name": "gonsai2",
  "version": "0.1.0",
  "description": "AI-Optimized Project with n8n and MongoDB integration",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:unit": "jest --selectProjects backend frontend --coverage",
    "test:integration": "jest tests/integration --coverage --coverageDirectory=coverage/integration",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "test:watch": "jest --watch",
    "test:changed": "jest --bail --findRelatedTests --onlyChanged --passWithNoTests",
    "test:coverage": "jest --coverage --coverageDirectory=coverage",
    "test:coverage:check": "jest --coverage --coverageThreshold='{\"global\":{\"branches\":70,\"functions\":70,\"lines\":70,\"statements\":70}}'",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "setup": "./setup-api-key.sh",
    "test:connection": "ts-node features/n8n-integration/test-connection.ts",
    "test:workflow": "ts-node features/n8n-integration/test-workflow-execution.ts",
    "test:websocket": "ts-node features/n8n-integration/test-websocket.ts",
    "test:agent": "ts-node features/agent-orchestration/tests/agent-manager.test.ts",
    "init:mongodb": "ts-node infrastructure/mongodb/scripts/initialize-db.ts",
    "test:mongodb": "ts-node infrastructure/mongodb/scripts/test-mongodb.ts",
    "server": "ts-node --project apps/backend/tsconfig.json apps/backend/src/server.ts",
    "server:dev": "nodemon",
    "dev": "ts-node index.ts",
    "build": "tsc -b",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "prepare": "husky install",
    "precommit": "lint-staged",
    "start:prod": "npm run build && pm2 start apps/backend/ecosystem.config.js --env production",
    "stop:prod": "pm2 stop gonsai2-backend",
    "restart:prod": "npm run build && pm2 restart gonsai2-backend --env production",
    "logs:prod": "pm2 logs gonsai2-backend",
    "delete:prod": "pm2 delete gonsai2-backend"
  },
  "keywords": [
    "n8n",
    "mongodb",
    "ai",
    "automation",
    "workflow"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@cypress/webpack-preprocessor": "^7.0.2",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/jest": "^30.0.0",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.10.1",
    "@types/nodemailer": "^7.0.4",
    "@types/supertest": "^6.0.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "@typescript-eslint/parser": "^8.46.4",
    "cypress": "^15.6.0",
    "eslint": "^9.39.1",
    "husky": "^9.1.7",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "jest-mock-extended": "^4.0.0",
    "lint-staged": "^16.2.6",
    "mongodb-memory-server": "^10.3.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/bull": "^3.15.9",
    "@types/cookie-parser": "^1.4.10",
    "@types/cron": "^2.0.1",
    "@types/jsonwebtoken": "^9.0.10",
    "bcryptjs": "^3.0.3",
    "bull": "^4.16.5",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "cron": "^4.3.4",
    "dotenv": "^16.0.0",
    "express": "^5.1.0",
    "express-rate-limit": "^8.2.1",
    "express-validator": "^7.3.0",
    "helmet": "^8.1.0",
    "ioredis": "^5.8.2",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^7.0.0",
    "morgan": "^1.10.1",
    "nodemailer": "^7.0.10",
    "redis": "^5.9.0",
    "socket.io": "^4.8.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.18.3",
    "ws": "^8.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
