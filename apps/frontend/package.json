{
  "name": "@gonsai2/frontend",
  "version": "0.1.0",
  "private": true,
  "description": "n8n Workflow Management Frontend",
  "scripts": {
    "dev": "next dev -p 3003 -H 0.0.0.0",
    "dev:local": "next dev -p 3003",
    "build": "next build",
    "start": "next start -p 3002",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:unit": "jest --config=jest.config.js",
    "test:integration": "jest --config=test/integration/jest.integration.config.js",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:load:workflow": "k6 run test/load/workflow-execution.js",
    "test:load:api": "k6 run test/load/api-load.js",
    "test:load:websocket": "k6 run test/load/websocket-load.js",
    "test:watch": "jest --watch",
    "test:coverage": "./scripts/test-coverage.sh",
    "optimize:db": "ts-node infrastructure/optimization/config/optimize-database.ts",
    "benchmark": "ts-node infrastructure/optimization/benchmarks/benchmark.ts",
    "cache:clear": "ts-node -e \"require('./infrastructure/optimization/cache/redis-client').default.flushAll()\"",
    "cache:stats": "ts-node scripts/cache-stats.ts",
    "perf:report": "ts-node scripts/performance-report.ts",
    "docs:dev": "docusaurus start",
    "docs:build": "docusaurus build",
    "docs:serve": "docusaurus serve",
    "docs:clear": "docusaurus clear",
    "docs:deploy": "docusaurus deploy"
  },
  "keywords": [
    "n8n",
    "workflow",
    "automation",
    "frontend"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "@tanstack/react-query": "^5.90.7",
    "@tanstack/react-query-devtools": "^5.90.2",
    "@tanstack/react-virtual": "^3.13.12",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "ioredis": "^5.4.1",
    "lucide-react": "^0.553.0",
    "mongodb": "^6.11.0",
    "next": "^15.5.7",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "reactflow": "^11.11.4",
    "recharts": "^3.4.1",
    "socket.io-client": "^4.8.1",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^3.4.18",
    "ws": "^8.18.3",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@playwright/test": "^1.49.1",
    "@types/express": "^5.0.1",
    "@types/ioredis": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^24.10.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@types/ws": "^8.5.13",
    "autoprefixer": "^10.4.22",
    "eslint": "^9.39.1",
    "eslint-config-next": "^16.0.1",
    "express": "^4.21.2",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "nyc": "^17.1.0",
    "postcss": "^8.5.6",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
